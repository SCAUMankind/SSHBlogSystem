<?xml version="1.0" encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >  
  
<hibernate-mapping package="com.blog.beans">  
    <class name="Place" table="place">  
        <id name="placeId" column="place_id">  
            <generator class="native"></generator>  
        </id>  
        <property name="province" column="province" type="java.lang.String"  />
        <property name="city" column="city" type="java.lang.String"  />
        <property name="area" column="area" type="java.lang.String"  />
    </class>  
</hibernate-mapping>  